            "hash": "1c5d1c48c998920a57359745180e7bbd548401939e6fb40fa5031e53dd512704"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/ANz308IT4pyDRtxmZJpWKiQ_91.0.4448.0/91.0.4448.0_chrome_installer.exe#/dl.7z",
            "hash": "d4bdbf24a433362a93be4405a936d2c23d0d7d562bfb4336410dc17ee73794f0"
        }
    },
    "installer": {
        "script": "Expand-7zipArchive \"$dir\\chrome.7z\" -ExtractDir 'Chrome-bin' -Removal"
