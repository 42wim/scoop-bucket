                "url": "https://dl.google.com/release2/chrome/$match64#/dl.7z",
                "hash": {
                    "url": "https://42wim.github.io/chromeupdates/chrome.min.xml",
                    "xpath": "/chromechecker/canary64[version='$version']/sha256"
