            "hash": "b173c21307daa979c228d6ce144d8f5252f3866997d0bebec68e890de4f0d45b"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/WjvG8wBq-0OykFubKvkQ3Q_93.0.4545.0/93.0.4545.0_chrome_installer.exe#/dl.7z",
